{
    "name": "spatie/laravel-url-signer",
    "description": "Laravel implementation of spatie/signed-url",
    "keywords": [
        "spatie",
        "laravel-url-signer"
    ],
    "homepage": "https://github.com/spatie/laravel-url-signer",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.1",
        "spatie/url-signer": "^2.0",
        "illuminate/support": "^9.0",
        "spatie/laravel-package-tools": "^1.13.6"
    },
    "require-dev": {
        "orchestra/testbench": "^7.12.1",
        "pestphp/pest": "^1.22.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\UrlSigner\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\UrlSigner\\Laravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\UrlSigner\\Laravel\\UrlSignerServiceProvider"
            ],
            "aliases": {
                "UrlSigner": "Spatie\\UrlSigner\\Laravel\\Facades\\UrlSigner"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
